{
  "name": "cloudops-dev-example",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "docker:clean": "docker-compose down --rmi local -v --remove-orphans",
    "docker:build": "docker build . -t cloudops-dev-example:latest",
    "docker:tag": "docker tag cloudops-dev-example:latest docker.registry.prod.xolv.app/cloudops-dev-example:$TAG",
    "docker:push": "docker push docker.registry.prod.xolv.app/cloudops-dev-example:$TAG",
    "docker:release": "yarn docker:build && yarn docker:tag && yarn docker:push",

    "start": "docker-compose up --remove-orphans --build",
    "prestart:clean": "yarn run docker:clean",
    "start:clean": "yarn run start",
    "test": "jest",
    "deploy": "groundhog deploy -a groundhog-testbed -e cloudops-dev-example"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~4.1.1",
    "express": "^4.16.3",
    "log4js": "^4.1.0",
    "log4js-sumologic-appender": "~2.0.1",
    "morgan": "~1.9.0",
    "pug": "^2.0.3",
    "serve-favicon": "~2.5.0"
  },
  "devDependencies": {
    "jest": "^24.7.1"
  }
}
